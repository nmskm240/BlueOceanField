{
    "name": "BlueOceanField",
    "build": {
        "dockerfile": ".dockerfile",
        "context": ".."
    },
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            },
            "extensions": [
                "ms-python.python",
                "njpwerner.autodocstring",
                "ms-python.black-formatter",
                "Postman.postman-for-vscode",
                "zxh404.vscode-proto3",
                "ms-python.isort",
                "EditorConfig.EditorConfig",
                "jebbs.plantuml"
            ]
        }
    },
}
